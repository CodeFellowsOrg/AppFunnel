<%= semantic_form_for @review do |f| %>
  <%= f.inputs do %>
    <%= f.input :education %>
    <%= f.input :contribution %>
    <%= f.input :resume %>
    <%= f.input :fit %>
    <%= f.input :work_experience %>
    <%= f.input :note %>
    <%= f.input :exceptional %>
  <% end %>

  <%= f.actions do %>
    <%= f.action :submit, :as => :input %>
  <% end %>
<% end %>
