<h2><small>Reviewing</small><br/><%= @apn.profile.first_name.capitalize + " ".to_s + @apn.profile.last_name.capitalize %></h2>
<div class="row">
  <div class="span8">
    <%= render 'profile_apn' %>
  </div>
  <div class="span4">
      <%= render 'form' %>
      <br>
      <h3>Class Registrations</h3>
      <% @apn.registrations.each do |registration| %>
        <%= render "course_registrations/registration", :registration => registration %>
      <% end %>


  </div>
</div>



